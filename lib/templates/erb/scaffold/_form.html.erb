<%%= form_for [:admin, @<%= file_name %>], html: { class: "form form-horizontal" } do |f| %>
  <div class="form-group">
    <label class="control-label">ID:</label>
    <%%= f.text_field :id, class: "form-control" %>
  </div>

  <input type="hidden" name="page" value="<%= @per_page %>" />
  <input type="hidden" name="q[name]" value="<%= @q %>" />

  <button type="submit" class="btn btn-default">Submit</button>
<%% end %>
